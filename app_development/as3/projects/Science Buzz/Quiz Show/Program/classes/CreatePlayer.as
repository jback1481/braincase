// $Id: createPlayer.as 443 2008-07-24 18:16:35Z jback $package {	public class CreatePlayer {		private var playerName:String;		private var score:Number;		private var scoreMe:Boolean;		private var answer:String;		private var feedback:String;		private var choiceFrame:Number;		private var colorFrame:Number;		public function CreatePlayer ():void {			playerName = "";			score = 0;			scoreMe = true;			answer = "";			feedback = "";			choiceFrame = 0;			colorFrame = 0;		}				public function givePoints ():void {				score = score + 1;		}				// Property access		public function get p_name ():String {			return playerName;		}		public function set p_name (setValue:String):void {			playerName = setValue;		}		public function get p_score ():Number {			return score;		}		public function set p_score (setValue:Number):void {			score = setValue;		}		public function get p_scoreMe ():Boolean {			return scoreMe;		}		public function set p_scoreMe (setValue:Boolean):void {			scoreMe = setValue;		}		public function get p_answer ():String {			return answer;		}		public function set p_answer (setValue:String):void {			answer = setValue;		}		public function get p_feedback ():String {			return feedback;		}		public function set p_feedback (setValue:String):void {			feedback = setValue;		}		public function get p_choiceFrame ():Number {			return choiceFrame;		}		public function set p_choiceFrame (setValue:Number):void {			choiceFrame = setValue;		}		public function get p_colorFrame ():Number {			return colorFrame;		}		public function set p_colorFrame (setValue:Number):void {			colorFrame = setValue;		}	}}